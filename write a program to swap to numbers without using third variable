//write a program to swap to numbers without using third variable
#include<stdio.h>
int main()
{
    int a;
    int b;
    
    printf("input the value of a and b \n");
    scanf("%d%d",&a,&b);
    
    printf("before swaping  value of a is %d and value of b is %d  \n" , a,b);
    a=a+b;
    b=a-b;
    a=a-b;
    
    printf("after swaping to numbers vlaue of a is %d and value of b is %d \n",a,b);
}
